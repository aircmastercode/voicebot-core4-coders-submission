# API Keys (Required to enable respective provider)
ANTHROPIC_API_KEY="your_anthropic_api_key_here"       # Required: Format: sk-ant-api03-...
PERPLEXITY_API_KEY="your_perplexity_api_key_here"     # Optional: Format: pplx-...
OPENAI_API_KEY="your_openai_api_key_here"             # Optional, for OpenAI/OpenRouter models. Format: sk-proj-...
GOOGLE_API_KEY="your_google_api_key_here"             # Optional, for Google Gemini models.
MISTRAL_API_KEY="your_mistral_key_here"               # Optional, for Mistral AI models.
XAI_API_KEY="YOUR_XAI_KEY_HERE"                       # Optional, for xAI AI models.
AZURE_OPENAI_API_KEY="your_azure_key_here"            # Optional, for Azure OpenAI models (requires endpoint in .taskmaster/config.json).
OLLAMA_API_KEY="your_ollama_api_key_here"             # Optional: For remote Ollama servers that require authentication.

# AWS Configuration
AWS_REGION="us-west-2"                               # AWS Region for API Gateway

# API Gateway Configuration
API_GATEWAY_URL="https://your-api-id.execute-api.us-west-2.amazonaws.com/dev"  # API Gateway URL
API_GATEWAY_KEY="your_api_key_here"                  # API Gateway API Key (if enabled)
API_GATEWAY_STAGE="dev"                              # API Gateway deployment stage

# S3 Configuration (Optional)
S3_BUCKET_NAME="your_bucket_name_here"               # S3 Bucket for storing knowledge base documents
S3_KB_PREFIX="knowledge-base/"                       # S3 Prefix for knowledge base documents

# Application Configuration
LOG_LEVEL="INFO"                                     # Logging level (DEBUG, INFO, WARNING, ERROR)
CACHE_TTL="3600"                                     # Cache time-to-live in seconds

# Workshop Configuration
TEAM_NAME="Core4 Coders"                             # Team name for workshop
TEAM_CODE="98e7-15c8f0-56"                          # Team code for workshop